<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Category</th>
      <th>Subcategory</th>
      <th>Tags</th>
      <th>Upvotes</th>
      <th>Media type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% articles.each do |article| %>
      <%= render article %>
    <% end %>
  </tbody>
</table>

<%= will_paginate articles %>
